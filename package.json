{
    "name": "ngx-bootstrap-animated-button-demo",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve -o",
        "build": "npm run pipeline:build:prd",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "clean-step:dist": "node lib/rimraf-standalone.js dist",
        "clean-step:node_modules": "node lib/rimraf-standalone.js node_modules",
        "build-step:dev": "npm run serve",
        "build-step:prd": "ng build --prod",
        "build-step:lint": "tslint --project tsconfig.json ./src/**/*.ts",
        "pipeline:clean": "npm run pipeline:clean:dev",
        "pipeline:clean:dev": "npm run clean-step:dist",
        "pipeline:clean:all": "npm run clean-step:dist && npm run clean-step:node_modules",
        "pipeline:build": "npm run pipeline:build:dev",
        "pipeline:build:dev": "npm run pipeline:clean:dev && npm run build-step:lint && npm run build-step:dev",
        "pipeline:build:prd": "npm run pipeline:clean:all && npm install && npm run build-step:lint && npm run build-step:prd"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~5.2.0",
        "@angular/common": "~5.2.0",
        "@angular/compiler": "~5.2.0",
        "@angular/core": "~5.2.0",
        "@angular/forms": "~5.2.0",
        "@angular/http": "~5.2.0",
        "@angular/platform-browser": "~5.2.0",
        "@angular/platform-browser-dynamic": "~5.2.0",
        "@angular/router": "~5.2.0",
        "@ngx-translate/core": "~10.0.0",
        "@ngx-translate/http-loader": "~3.0.0",
        "angular-font-awesome": "^3.1.2",
        "bootstrap": "~4.1.0",
        "core-js": "~2.4.0",
        "font-awesome": "^4.7.0",
        "jquery": "~3.3.0",
        "rxjs": "~5.5.0",
        "zone.js": "~0.8.0"
    },
    "devDependencies": {
        "@angular/cli": "~1.7.0",
        "@angular/compiler-cli": "~5.2.0",
        "@angular/language-service": "~5.2.0",
        "@types/jasmine": "~2.8.0",
        "@types/jasminewd2": "~2.0.0",
        "@types/node": "~6.0.0",
        "codelyzer": "~4.0.0",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.0",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~1.2.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "~0.2.0",
        "protractor": "~5.1.0",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.0",
        "typescript": "~2.5.0"
    }
}
